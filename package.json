{
    "name": "demoblazefer",
    "version": "1.0.0",
    "description": "Proyecto de prueba para ejecuciones de automation con cypress.",
    "main": "index.js",
    "scripts": {
        "test": "npm run recordTest",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "headTest": "npm run cy:run -- --headed",
        "chromeTest": "npm run cy:run --parallel",
        "recordTest": "cypress run --record --key 1be363fb-4b41-494c-ad5e-89c43534d398 --reporter mochawesome",
        "homeTest": "cypress run -- --spec \"cypress/integration/testsHome/*\"",
        "start-jenkins-server": "java -jar jenkins.war --httpPort=9090 --enable-future-java",
        "del:reports": "rm cypress/results/* || true",
        "report:exec": "cypress run --reporter mochawesome",
        "report:merge": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json",
        "report:generate": "npx marge mochawesome.json",
        "junit:del": "rm -R results || true",
        "delete:reports": "rm cypress/results/* || true",
        "delete:mocha": "rm -R mochawesome-report || true",
        "delete:mochajson": "rm mochawesome.json || true",
        "report": "npm run del:reports && npm run report:exec && npm run report:merge && report:generate"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true
    },
    "author": "Fernando J. Arroup√©",
    "license": "ISC",
    "devDependencies": {
        "@cucumber/cucumber": "^7.3.1",
        "@types/async": "2.0.41",
        "@types/chokidar": "^1.7.0",
        "@types/connect": "3.4.30",
        "@types/fs-extra": "4.0.0",
        "@types/glob": "5.0.31",
        "@types/jasmine": "3.3.13",
        "chai": "4.1.1",
        "chai-as-promised": "7.1.1",
        "connect": "^3.7.0",
        "cucumber": "2.3.1",
        "cucumberjs-junitxml": "1.0.0",
        "cypress": "^5.6.0",
        "cypress-cucumber-preprocessor": "^4.2.0",
        "cypress-file-upload": "^5.0.8",
        "https-proxy-agent": "^5.0.0",
        "jasmine-spec-reporter": "^4.2.1",
        "jasmine-ts": "^0.2.1",
        "mocha": "^8.4.0",
        "mochawesome": "^6.2.2",
        "mochawesome-merge": "^4.2.0",
        "mochawesome-report-generator": "^5.2.0",
        "prettier": "2.1.2",
        "protractor": "^6.0.0",
        "protractor-cucumber-framework": "3.1.2",
        "serve-static": "1.13.2",
        "shortid": "2.2.8",
        "ts-node": "5.0.1",
        "tslint": "5.6.0",
        "tslint-eslint-rules": "4.1.1",
        "typescript": "2.4.2",
        "vrsource-tslint-rules": "5.1.1"
    },
    "dependencies": {
        "angular": "1.6.6",
        "angular-mocks": "1.6.6",
        "angular-resource": "1.6.6",
        "async": "2.5.0",
        "chokidar": "^3.5.2",
        "fs-extra": "4.0.1",
        "glob": "7.1.2",
        "hooker": "0.2.3",
        "lodash": "^4.17.21",
        "path": "0.12.7",
        "superagent": "^6.1.0",
        "superagent-proxy": "^2.0.0",
        "then-request": "4.1.0",
        "url-join": "2.0.2",
        "uuid": "3.1.0"
    }
}
